{
    "name": "groupvan/server-sdk",
    "description": "Official PHP server SDK for GroupVAN V3 API authentication",
    "type": "library",
    "homepage": "https://github.com/federatedops/GroupVAN-SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "GroupVAN"
        }
    ],
    "require": {
        "php": ">=8.1",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.0",
        "phpseclib/phpseclib": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GroupVAN\\ServerSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GroupVAN\\ServerSDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse",
        "keygen": "php Client.php"
    },
    "support": {
        "issues": "https://github.com/federatedops/GroupVAN-SDK/issues",
        "source": "https://github.com/federatedops/GroupVAN-SDK"
    },
    "bin": [
        "bin/groupvan-server-keygen"
    ]
}